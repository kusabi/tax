{
    "name": "kusabi/tax",
    "description": "A library for calculating tax in PHP",
    "type": "library",
    "license": "mit",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Christian Harvey",
            "email": "kusabi.software@gmail.com"
        }
    ],
    "keywords": [
        "php",
        "tax"
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "*",
        "nesbot/carbon": "*",
        "phpunit/phpunit": "*",
        "symfony/var-dumper": "*",
        "phpbench/phpbench": "*"
    },
    "autoload": {
        "files": [
            "src/tax.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Kusabi\\Tax\\Benchmarks\\": "benchmarks/",
            "Kusabi\\Tax\\Tests\\": "tests/"
        }
    }
}
